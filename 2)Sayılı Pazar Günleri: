d = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31];

s = 1 + 365;
ans = 0;
for (y = 1901; y <= 2000; y++) {
  for (m = 0; m < 12; m++) {
    if (s % 7 == 0) ans++;
    s += d[m];
    if (m == 1 && y % 4 == 0) {
      if (y % 100 != 0 || y % 400 == 0) {
        s += 1;
      }
    }
  }
}

document.write('ANS: ', ans);
